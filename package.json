{
	"name": "svelte-image-editor",
	"description": "svelte image editor",
	"version": "0.1.0",
	"license": "(MIT OR Apache-2.0)",
	"type": "module",
	"author": "Nathan Faucett",
	"repository": {
		"type": "git",
		"url": "https://github.com/nathanfaucett/svelte-image-editor"
	},
	"homepage": "https://github.com/nathanfaucett/svelte-image-editor#readme",
	"keywords": [
		"svelte",
		"image",
		"editor"
	],
	"scripts": {
		"svelte-kit": "svelte-kit",
		"vite": "vite",
		"start": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"clean": "rm -rf node_modules/.vite .svelte-kit package build",
		"package": "svelte-package",
		"publish": "npm run package && cd package && npm publish --access public",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@types/gl-matrix": "3",
		"@typescript-eslint/eslint-plugin": "5",
		"@typescript-eslint/parser": "5",
		"eslint": "8",
		"eslint-config-prettier": "8",
		"eslint-plugin-svelte3": "4",
		"prettier": "2",
		"prettier-plugin-svelte": "2",
		"svelte": "3",
		"svelte-check": "2",
		"svelte-icons": "2",
		"svelte-portal": "2",
		"svelte-preprocess": "4",
		"svelte2tsx": "0",
		"tslib": "2",
		"typescript": "4"
	},
	"peerDependencies": {
		"svelte": "3",
		"tslib": "2"
	},
	"dependencies": {
		"@use-gesture/vanilla": "10",
		"gl-matrix": "3",
		"@svelte-put/clickoutside": "1"
	}
}
