{
	"name": "svelte-image-editor",
	"description": "svelte image editor",
	"version": "0.1.0",
	"license": "(MIT OR Apache-2.0)",
	"type": "module",
	"author": "Nathan Faucett",
	"repository": {
		"type": "git",
		"url": "https://github.com/nathanfaucett/svelte-image-editor"
	},
	"homepage": "https://github.com/nathanfaucett/svelte-image-editor#readme",
	"keywords": [
		"svelte",
		"image",
		"editor"
	],
	"scripts": {
		"start": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"clean": "rm -rf node_modules/.vite .svelte-kit package",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"publish": "npm run package && cd package && npm publish --access public",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@types/gl-matrix": "3",
		"@typescript-eslint/eslint-plugin": "5",
		"@typescript-eslint/parser": "5",
		"eslint": "8",
		"eslint-config-prettier": "8",
		"eslint-plugin-svelte3": "3",
		"prettier": "2",
		"prettier-plugin-svelte": "2",
		"svelte": "3",
		"svelte-check": "2",
		"svelte-icons": "2",
		"svelte-portal": "2",
		"svelte-preprocess": "4",
		"svelte2tsx": "0",
		"tslib": "2",
		"typescript": "4"
	},
	"peerDependencies": {
		"svelte": "3",
		"tslib": "2"
	},
	"dependencies": {
		"@use-gesture/vanilla": "10",
		"gl-matrix": "3",
		"svelte-use-click-outside": "1"
	}
}
